FAKE(meshgrid(linspace(-1.0,1.0,200), linspace(-1.0, 1.0, 200)))
SCRIPT({
  math := import("math")
  ctx := import("context")
  values := ctx.value()
  x := values[0]
  y := values[1]
  z := math.sin(10*(math.pow(x,2) + math.pow(y,2)))/10
  ctx.yieldKey(x, y, z)
})
CHART_LINE3D(
  size('800px', '600px'),
  lineWidth(4), 
  gridSize(100, 30, 100), 
  visualMap(-0.12, 0.12), 
  autoRotate(20)
)